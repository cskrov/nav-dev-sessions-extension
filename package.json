{
  "name": "nav-dev-sessions-extension",
  "version": "2.0.4",
  "description": "Browser extension to map session cookies to localhost.",
  "scripts": {
    "build": "bun bun.build.ts",
    "dev": "bun run build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "chrome:pack": "web-ext build --overwrite-dest --source-dir=dist/chrome --artifacts-dir=dist --filename={name}-{version}-chrome.zip",
    "firefox:pack": "web-ext build --overwrite-dest --source-dir=dist/firefox --artifacts-dir=dist --filename={name}-{version}-firefox.zip",
    "firefox:sign": "web-ext sign --source-dir=dist/firefox --artifacts-dir=dist --channel=unlisted --upload-source-code=source-package.zip",
    "generate-update-manifest": "bun run scripts/generate-update-manifest.ts"
  },
  "license": "MIT",
  "dependencies": {
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tailwindcss": "4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@iconify/react": "6.0.2",
    "@types/bun": "1.3.6",
    "@types/chrome": "0.1.35",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@types/webextension-polyfill": "0.12.4",
    "bun-plugin-tailwind": "0.1.2",
    "typescript": "5.9.3",
    "web-ext": "9.2.0",
    "webextension-polyfill": "0.12.0"
  }
}
